<%= form_for(@book) do |f| %>
<div class="field">
    <%= f.label :isbn %> <br />
    <%= f.text_filed :isbn , size: 25 %>
</div>

<%= form_tag(action: :create) do %>
<div class="field">
    <%= label :book, :isbn %><br />
    <%= text_field :book, :isbn, size: 25 %>
    </div>
    
<div class="field">
    <%=label :book, :title %> <br />
    <%= text_field :book, :title, size: 25 %>
</div>

<div class="actions">
    <%= f.submit %>
</div>

<% end %>